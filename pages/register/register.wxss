@import "../../wxParse/wxParse.wxss"; 
page{
	width:100%;
	min-height:100%;
	
}
.register-title{
	width:100%;
	text-align: center;
	height:90rpx;
	line-height: 90rpx;
}
.register-content{
	width:92%;
	padding: 0 4%;
}
.form{
	width:92%;
	margin:50rpx auto;
	height:730rpx;
	border: 1px solid #f4f4f4;
	border-radius:10rpx;
	box-shadow:0 0 1px #FF7F23;
}
.form-item{
	width:92%;
	height:90rpx;
	line-height: 90rpx;
	border-bottom: 1px solid #f4f4f4;
	padding: 0 4%;
}
.submit{
	width:100%;
	height:180rpx;
}
.place-holder {
	 /* placeholder颜色  */
	 color:rgba(45,15,6,0.6);
	font-size:24rpx
 }
 .form-item-title{
	 width:25%
 }
 .form-item input{
	 width:75%
 }