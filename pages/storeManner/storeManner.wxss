/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
page{
	width:100%;
	min-height:100%;
	
}
.tab{
	height:80rpx;
	border-bottom:solid 1px #f4f4f4;
}
.tab_item{
	width:50%;
	text-align:center;
	height:80rpx;
	line-height:80rpx;
}
.tab_this{
	color:#FF7F23;
	border-bottom:solid 2px #FF7F23;
}
.tab_item text{
	font-size: 28rpx;
	height:78rpx;
	line-height:78rpx;
	display:inline-block;
}
.form{
	width:100%;

}
.form-item{
	
	height:90rpx;
	line-height: 90rpx;
	border-bottom: 1px solid #f4f4f4;

}
 .form-item-title{
	 width:25%;
	 margin-left:4%
 }
 .form-item input{
	 width:75%
 }
 .store-product{
 	
 	width:92%;
 	padding: 0 4%;
 }
 .group-main{
 	width:100%;
 	height:270rpx;
 	background: #fff;
 	border-radius:15rpx;

 }
 .group-main-left{
 	width:50%;
 	height:100%;
 }
 .group-main-right{
 	width:50%;
 	height:80%;
 }
 .group-main-left image{
 	width:90%;
 	height:80%;
 }

