/* groupStore.wxss */
.form-item{
	width:92%;
	height:130rpx;
	
	border-bottom: 10px solid #f4f4f4;
	padding: 0 4%;
}
.form-item-title{
	 width:100%
 }
 .form-item-title-img image{
	width:37rpx;
	height:33rpx;

 }
 .search{
 	width:100%;
 	height:100rpx;
 	line-height: 100rpx;
 	border-bottom: 1px solid #f4f4f4;
 }
 .search-input{
 	width:100%;
 	position: relative;
 }
 .search-input input{
 	width:75%;
 	margin: 0 4%;
 	height:60rpx
 }
 .search-input image{
 	width:30rpx;
 	height:30rpx;
 	position: absolute;
 	top:19rpx;
 	left:50rpx
 }